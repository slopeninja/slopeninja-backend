<script id="liftStatus_template" type="text/template">
    <div>
        <% _.each(lifts, function(lift) { %>
        <div class="liftStatus__lifts__row c118__name--v1" tabindex="0">
            <div class="liftStatus__lifts__row--icon <%= lift.statusIcon %>"></div>
            <div class="liftStatus__lifts__row--icon <%= lift.liftIconClass %>"></div>
            <div class="sr-only"><%= lift.Type %> </div>
            <div class="liftStatus__lifts__row--name">
                <span class="liftStatus__lifts__row__title"><%= lift.Name %></span>
                <span class="liftStatus__lifts__row__title liftStatus__lifts__row__time <%= !lift.OpenTime && 'hidden' %>"><%= moment(lift.OpenTime, "h:mm a").format("LT") %> - <%= moment(lift.CloseTime, "h:mm A").format("LT") %></span>
            </div>
            <div class="sr-only"><%= lift.Status %></div>
        </div>
        <% }); %>
    </div>
</script>
<script id="liftStatusFilter_template" type="text/template">
    <div>
        <% _.each(filters, function(filter) { %>
        <div class="liftStatus__filter__value c118__filtervalue--v1">
            <div class="checkbox checkbox--custom liftStatus__filter__value--checkbox">
                <label>
                    <span class="statusIcon <%= filter.icon %>" aria-hidden="true"></span>
                    <span class="liftStatus__filter_value--labelText"><%= filter.DisplayText %></span>
                </label>
            </div>
        </div>
        <% }); %>
    </div>
</script>







