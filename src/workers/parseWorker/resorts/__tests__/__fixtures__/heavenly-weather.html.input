<script id="header_weather__forecast" type="text/template">
    <div class="row header_weather__forecast__top_row">
        <div class="col-xs-8 c126__title--v1 row header_weather__title header_weather__title--forecast uppercase"><span class="header_weather__title_icon header_weather__title_icon--forecast icon-weather-todays-forecast"></span><%= (weather.HeaderSettings.DailyForecastTitle) %></div> 
        <div class="col-xs-4 header_weather__measurement_toggle">
            <a href="javascript:void(0);" class="header_weather__measurement header_weather__measurement--fahrenheit <%= ((weather.UnitOfMeasure == 1)? 'c126__metricselected--v1' : 'c126__metricunselected--v1' ) %>" data-component-element="hdrWeatherHeader_Units_Link" data-component-units="fahrenheit" data-track="click">F&#176;<span class="sr-only">, Click here for Fahrenheit</span></a>
            <a href="javascript:void(0);" class="header_weather__measurement header_weather__measurement--celsius <%= ((weather.UnitOfMeasure == 1)? 'c126__metricunselected--v1' : 'c126__metricselected--v1') %>" data-component-element="hdrWeatherHeader_Units_Link" data-component-units="celsius" data-track="click">C&#176;<span class="sr-only">, Click here for Celsius</span></a>
        </div>
    </div>
    <div class="row header_weather__forecast__container">
        <div class="col-xs-6 col-sm-7 col-md-8 col-lg-6 header_weather__forecast__weather_container">
            <div class="header_weather__forecast__description_container">
                <div class="c126__temp1--v1 header_weather__forecast__weather">
                    <i class="header_weather__forecast_icon" aria-hidden="true"></i>
                    <span class="tempnumber header_weather__us_only"><%= (weather.CurrentTempStandard) %></span><span class="header_weather__tempunit header_weather__us_only"></span>
                    <span class="tempnumber header_weather__international_only"><%= (weather.CurrentTempMetric) %></span><span class="header_weather__tempunit header_weather__international_only"></span>
                </div>
                <div class="header_weather__forecast__description c126__shortdescription1--v1"><%= (weather.WeatherShortDescription) %></div>
            </div>
        </div>
        <div class="col-xs-6 col-sm-5 col-md-4 col-lg-6 header_weather__forecast__hi_low_container">
            <div class="header_weather__forecast_hi">
