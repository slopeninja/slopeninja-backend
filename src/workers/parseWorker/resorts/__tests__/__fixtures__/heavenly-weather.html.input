<script id="forecastTodayTemplate" type="text/template">
    <h3 class="c78__title--v3 forecast__today__title">
        Today&#39;s Forecast
    </h3>
    <div class="forecast__today__sponsor">
        <% if (location.SponsorImageUrl) { %>
            <a href="<%= location.SponsorImageUrl %>" target="_blank">
                <%= location.image %>
            </a>
        <% } else { %>
            <%= location.image %>
        <% } %>
        <span class="forecast__today__sponsor_copy c78__timestamp--v1"><%= location.SponsorCopy %></span>
    </div>
    <ul class="forecast__today__primary">
        
        <li class="c78__temp1--v1 forecast__today__weather">
            <ul>
                <li class="forecast__today__weather__container">
                    <i class="icon-weather-<%= today.icon %>"></i>
                    <%= today.temp.current %><span class="forecast__today__weather__degrees"><%= tempScale %></span>
                </li>
                <li class="col-xs-12 c78__shortdescription1--v1 forecast__today__weather__description">
                    <%= today.shortDescription %>
                </li>
            </ul>
        </li>

        
        <li class="forecast__today__temps  <%= today.snow ? '' : 'forecast--no_border'%> ">
            <ul>
                <li class="forecast__today__temps__hi">
                    <div class="c78__temp2--v1 forecast__today__temps__hi__temp">
                        <%= today.temp.high %><span class="forecast__today__temps__hi__degrees"><%= tempScale %></span>
                    </div>
                    <div class="c78__hilow1--v1 forecast__today__temps__hi__label">
                        <i class="icon-weather-arrow-hi"></i>
                        hi
                    </div>
                </li>
                <li class="forecast__today__temps__low">
                    <div class="c78__temp2--v1 forecast__today__temps__low__temp">
                        <%= today.temp.low %><span class="forecast__today__temps__low__degrees"><%= tempScale %></span>
                    </div>
                    <div class="c78__hilow1--v1 forecast__today__temps__low__label">
                        <i class="icon-weather-arrow-low"></i>
                        low
                    </div>
                </li>
            </ul>
        </li>

            <% if (today.snow) { %>
            <li class="forecast__today__daytime ">
                <ul class="forecast__today__daytime_container">
                    <li class="c78__total2--v1 forecast__today__daytime__snow">
                        <%= today.snow.daytime %><span class="forecast__length_scale"><%= (today.snow.daytime.toLowerCase() !== 'nil') ? lengthScale : '' %></span>
                    </li>
                    <li class="c78__label1--v1 forecast__today__daytime__label">
                        Daytime snow
                    </li>
                </ul>
            </li>
            <% } %>

            <% if (today.snow) { %>
            <li class="forecast__today__overnight ">
                <ul class="forecast__today__overnight_container">
                    <li class="c78__total2--v1 forecast__today__overnight__snow">
                        <%= today.snow.overnight %><span class="forecast__length_scale"><%= (today.snow.overnight.toLowerCase() !== 'nil') ? lengthScale : '' %></span>
                    </li>
                    <li class="c78__label1--v1 forecast__today__overnight__label">
                        Overnight snow
                    </li>
                </ul>
            </li>
            <% } %>
        <li class="c78__longdescription2--v1 forecast__today__description<%= today.longDescription ? '' : ' visible-lg' %>">
            <%= today.longDescription %>
        </li>
    </ul>

    <ul class="forecast__secondary">
        
        <% if (location.elevation || location.elevation === 0) { %>
        <li class="forecast__secondary__item">
            <i class="forecast__secondary__item_icon icon-weather-elevation"></i>
            <div class="forecast__secondary__item_body">
                <span class="c78__conditions--v1">
                    Elevation:
                </span>
                <span class="c78__conditions--v2">
                    <%= location.ElevationText %> <%= location.elevation %><%= heightScale %>
                </span>
            </div>
        </li>
        <% } %>

        
        <% if (today.wind) { %>
        <li class="forecast__secondary__item">
            <i class="forecast__secondary__item_icon icon-weather-windy"></i>
            <div class="forecast__secondary__item_body">
                <span class="c78__conditions--v1">
                    Winds:
                </span>
                <span class="c78__conditions--v2">
                    <%= today.wind %>
                </span>
            </div>
        </li>
        <% } %>

        
        <% if (today.freezingLevel !== null) { %>
        <li class="forecast__secondary__item">
            <i class="forecast__secondary__item_icon icon-weather-snowflake"></i>
            <div class="forecast__secondary__item_body">
                <span class="c78__conditions--v1">
                    Freezing Level:
                </span>
                <span class="c78__conditions--v2">
                    <%= today.freezingLevel %><%= heightScale %>
                </span>
            </div>
        </li>
        <% } %>
    </ul>
</script>

<script id="forecastAccordionTemplate" type="text/template">
    <% _.each(forecasts, function(forecast, index) { %>

    
    <a class="forecast__accordion__btn collapsed-text <%= forecast.opened ? '' : 'collapsed' %>"
       title="ViewMore"
       href="javascript:void(0);"
       data-target="#weatherAhead<%= index %>"
       data-toggle="collapse"
       aria-expanded="false"
       aria-controls="weatherAhead<%= index %>"
       data-component-element="bdyWeatherPageForecast_Day_Accordion"
       data-component-date-month-name="<% if(typeof forecast.date.month !== 'undefined'){ %><%= forecast.date.month.toLowerCase() %><% }; %>"
       data-component-date-day-name="<% if(typeof forecast.date.longName !== 'undefined'){ %><%= forecast.date.longName.toLowerCase() %><% }; %>"
       data-component-date-day-number="<% if(typeof forecast.date.number !== 'undefined'){ %><%= forecast.date.number %><% }; %>"
       data-track="click">

        <div class="forecast__accordion__item__container">
            
            <div class="c78__month1--v1 forecast__accordion__date">
                <%= forecast.date.month %>
                <span class="c78__date1--v1 forecast__accordion__date__number"><%= forecast.date.number %></span>
                <span class="c78__day1--v1 forecast__accordion__date__day"><%= forecast.date.shortName %></span>
            </div>


            <ul class="c78__temp4--v1 forecast__accordion__item">
                
                <li class="c78__day1--v1 forecast__accordion__day">
                    <%= forecast.date.longName %>
                </li>

                
                <li class="forecast__accordion__icon">
                    <i class="icon-weather-<%= forecast.icon %>"></i>
                </li>

                
                <li class="c78__temp4--v1 forecast__accordion__hi">
                    <div class="forecast__accordion__hi__container">
                        <%= forecast.temp.high %><span class="forecast__today__weather__degrees"><%= tempScale %></span>
                        <span class="c78__hilow2--v1 forecast__accordion__hi__label">hi</span>
                    </div>
                </li>

                
                <li class="c78__temp4--v1 forecast__accordion__lo">
                    <div class="forecast__accordion__lo__container">
                        <%= forecast.temp.low %><span class="forecast__today__weather__degrees"><%= tempScale %></span>
                        <span class="c78__hilow2--v1 forecast__accordion__lo__label">low</span>
                    </div>
                </li>


                
                <li id="weatherAhead<%= index %>"
                    class="c78__temp4--v1 collapse forecast__accordion__body <%= forecast.opened ? 'in' : '' %>">

                    <ul class="forecast__accordion__body__details
                        
                        <%= forecast.snow ? '' : 'forecast__accordion__body__details__empty' %>">

                        
                        <li class="forecast__accordion__body__hi">
                            <div class="c78__temp1--v1 forecast__accordion__body__hi__temp">
                                <%= forecast.temp.high %><span class="forecast__today__weather__degrees"><%= tempScale %></span>
                                <div class="c78__hilow1--v1 forecast__accordion__body__hi__label">
                                    <i class="icon-weather-arrow-hi"></i>
                                    hi
                                </div>
                            </div>
                        </li>

                        
                        <li class="c78__temp1--v1 forecast__accordion__body__lo">
                            <div class="c78__temp1--v1 forecast__accordion__body__lo__temp">
                                <%= forecast.temp.low %><span class="forecast__today__weather__degrees"><%= tempScale %></span>
                                <div class="c78__hilow1--v1 forecast__accordion__body__lo__label">
                                    <i class="icon-weather-arrow-low"></i>
                                    low
                                </div>
                            </div>
                        </li>

                            <% if (forecast.snow) { %>
                                <li class="c78__total2--v1 forecast__accordion__body__daytime <%= (_.isNull(forecast.snow.overnight) || _.isUndefined(forecast.snow.overnight)) ? 'forecast__accordion__body__daytime--no_right_border' : ''  %>">
                                    <ul>
                                        <li class="c78__total2--v1 forecast__accordion__body__daytime__snow">
                                            <%= forecast.snow.daytime %><span class="forecast__length_scale"><%= (forecast.snow.daytime.toLowerCase() !== 'nil') ? lengthScale : '' %></span>
                                        </li>
                                        <li class="c78__label1--v1 forecast__accordion__body__daytime__label">
                                            <%= (_.isNull(forecast.snow.overnight) || _.isUndefined(forecast.snow.overnight)) ? 'Accumulation' : 'Daytime snow'  %>
                                        </li>
                                    </ul>
                                </li>
                            <% } %>

                            <% if (forecast.snow && !_.isNull(forecast.snow.overnight) && !_.isUndefined(forecast.snow.overnight)) { %>
                                <li class="forecast__accordion__body__overnight">
                                    <ul>
                                        <li class="c78__total2--v1 forecast__accordion__body__overnight__snow">
                                            <%= forecast.snow.overnight %><span class="forecast__length_scale"><%= (forecast.snow.overnight.toLowerCase() !== 'nil') ? lengthScale : '' %></span>
                                        </li>
                                        <li class="c78__label1--v1 forecast__accordion__body__overnight__label">
                                            Overnight snow
                                        </li>
                                    </ul>
                                </li>
                            <% } %>

                    </ul>

                    <ul class="forecast__secondary">
                        
                        <% if (forecast.wind) { %>
                        <li class="forecast__secondary__item">
                            <i class="forecast__secondary__item_icon icon-weather-windy"></i>
                            <div class="forecast__secondary__item_body">
                                <span class="c78__conditions--v1">
                                    Winds:
                                </span>
                                <span class="c78__conditions--v2">
                                    <%= forecast.wind %>
                                </span>
                            </div>
                        </li>
                        <% } %>

                        <% if (forecast.freezingLevel !== null) { %>
                        
                        <li class="forecast__secondary__item">
                            <i class="forecast__secondary__item_icon icon-weather-snowflake"></i>
                            <div class="forecast__secondary__item_body">
                                <span class="c78__conditions--v1">
                                    Freezing Level:
                                </span>
                                <span class="c78__conditions--v2">
                                    <%= forecast.freezingLevel %><%= heightScale %>
                                </span>
                            </div>
                        </li>
                        <% } %>
                    </ul>

                    
                    <div class="forecast__accordion__body__description c78__longdescription2--v1">
                        <%= forecast.description %>
                    </div>
                </li>
            </ul>
        </div>
    </a>
    <% }); %>
</script>
