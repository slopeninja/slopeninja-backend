<script type="text/template" id="snow_report_template">
    <% if(!mappedData.hideSnowReport || false){ %>
    <div class="snow_report__section row">
        <div class="snow_report__header col-xs-12">
            <h2 class="snow_report__header__title c78__title--v1 col-xs-6">Snow Report</h2>
            <ul class="snow_report__header__units col-xs-6">
                <li class="snow_report__header__unit <% if(mappedData.selectedUnits === 'inches') { %>c78__metricselected--v1<% } else { %>c78__metricunselected--v1<% }; %>" data-component-element="bdyWeatherPageSnowReport_Units_Toggle" data-component-units="inches" data-track="click"><a class="snow_report__header__link" data-units="inches" href="#" title="Inches">Inches</a></li>
                <li class="snow_report__header__unit <% if(mappedData.selectedUnits === 'cm') { %>c78__metricselected--v1<% } else { %>c78__metricunselected--v1<% }; %>" data-component-element="bdyWeatherPageSnowReport_Units_Toggle" data-component-units="cm" data-track="click"><a class="snow_report__header__link" data-units="cm" href="#" title="CM">CM</a></li>
            </ul>
        </div>
    </div>
    <div class="snow_report__content row">
        <ul class="snow_report__metrics col-xs-12">

            <% if(mappedData.title && mappedData.description) { %>
            <li class="snow_report__metrics__metric">
                <div class="snow_report__metrics__metric_group">
                    <h5 class="snow_report__metrics__title c78__conditions1--v1"><%= mappedData.title %></h5>
                    <h6 class="snow_report__metrics__description c78__label1--v1"><%= mappedData.description %></h6>
                </div>
            </li>
             <% } %>

            <% _.each(mappedData.metrics, function(metric) { %>
            <li class="snow_report__metrics__metric">
                <div class="snow_report__metrics__metric_group">
                    <h5 class="snow_report__metrics__measurement c78__total1--v1"><% if(mappedData.selectedUnits === 'cm') { %><%= metric.cm %><span>cm</span><% } else { %><%= metric.inches %><span>in</span><% }; %></h5>
                    <h6 class="snow_report__metrics__description c78__label1--v1"><%= metric.context %></h6>
                </div>
            </li>
            <% }); %>
        </ul>
        <p class="snow_report__metrics__timestamp c78__timestamp--v1 col-xs-12">
            <span class="snow_report__metrics__timestamp_clock icon-booking-clock"></span>
            Updated December 20, 2020 at 6:26 AM PST
        </p>
    </div>
    <% }; %>
</script>

